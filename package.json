{
  "name": "svcs-api-vitals",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:docker": "(npm run migration) & npm run stepfunction-init & nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "TZ=UTC jest",
    "test:watch": "jest --watch",
    "docs:generate": "ts-node ./generateMdDocs.ts && ./node_modules/.bin/compodoc -p tsconfig.json -d ./docs --theme laravel --hideGenerator --disableDependencies --includes ./",
    "docs:serve": "./node_modules/.bin/compodoc -d ./docs -s --includes ./",
    "docs": "ts-node ./generateMdDocs.ts && ./node_modules/.bin/compodoc -p tsconfig.json -s -w -r 8080 -d ./docs --theme laravel --hideGenerator --disableDependencies --includes ./",
    "test:cov": "TZ=UTC jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "TZ=UTC jest --config ./test/jest-e2e.json",
    "stepfunction-init": "aws stepfunctions --endpoint http://stepfunction:4584 create-state-machine --name observation-ingestion-local --definition '{\"Comment\":\"A Hello World example of the Amazon States Language using a Pass state\",\"StartAt\":\"HelloWorld\",\"States\":{\"HelloWorld\":{\"Type\":\"Pass\",\"End\":true}}}' --role-arn arn:aws:iam::0123456789:role/service-role/MyRole",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts",
    "migration": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/ormconfig.ts migration:run",
    "migration:undo": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/ormconfig.ts migration:revert",
    "migration:generate": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/ormconfig.ts migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/ormconfig.ts migration:create -n",
    "sync": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/database.config.ts schema:sync",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.1",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/event-emitter": "1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/terminus": "^7.2.0",
    "@nestjs/typeorm": "^8.0.2",
    "@sendgrid/mail": "^7.4.6",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.172",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.971.0",
    "aws-sdk-client-mock": "^0.5.3",
    "axios-retry": "^3.1.9",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cron-time-generator": "^1.2.0",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express-basic-auth": "^1.2.0",
    "faker": "^5.5.3",
    "fast-json-patch": "^3.1.0",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.33",
    "nestjs-i18n": "^8.1.4",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6",
    "tsconfig-paths": "^3.10.1",
    "twilio": "^3.67.0",
    "typeorm": "^0.2.29",
    "typeorm-factories": "^1.0.1",
    "typeorm-naming-strategies": "^2.0.0",
    "typescript": "^4.3.5",
    "uuidv4": "^6.2.12",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.14",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.6",
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.6.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "mockdate": "^3.0.5",
    "prettier": "2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "rootDir": "./",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "./jest-setup-file.ts"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "volta": {
    "node": "14.15.1",
    "npm": "6.14.9"
  }
}
