  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:docker": "(npm run migration) & npm run stepfunction-init & nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/ormconfig.ts migration:run",
    "migration:undo": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/ormconfig.ts migration:revert",
    "migration:generate": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/ormconfig.ts migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/ormconfig.ts migration:create -n",
    "sync": "ts-node ./node_modules/typeorm/cli -f ./src/database/config/database.config.ts schema:sync"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.1",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/event-emitter": "1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/terminus": "^7.2.0",
    "@nestjs/typeorm": "^8.0.2",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.172",
    "axios-retry": "^3.1.9",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "express-basic-auth": "^1.2.0",
    "fast-json-patch": "^3.1.0",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6",
    "tsconfig-paths": "^3.10.1",
    "typeorm": "^0.2.29",
    "typeorm-factories": "^1.0.1",
    "typeorm-naming-strategies": "^2.0.0",
    "typescript": "^4.3.5",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.6",
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.6.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
